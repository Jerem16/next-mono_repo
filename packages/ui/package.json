{
    "name": "@packages/ui",
    "version": "0.1.0",
    "private": true,
    "type": "module",
    "main": "dist/index.js",
    "types": "dist/index.d.ts",
    "exports": {
        ".": {
            "types": "./dist/index.d.ts",
            "default": "./dist/index.js"
        },
        "./auth": {
            "types": "./dist/auth/index.d.ts",
            "default": "./dist/auth/index.js"
        }
    },
    "typesVersions": {
        "*": {
            "auth": [
                "src/auth/index.ts"
            ],
            "*": [
                "src/*"
            ]
        }
    },
    "scripts": {
        "build": "tsc --build",
        "tsc": "tsc --noEmit",
        "lint": "eslint --config ../../eslint.config.mjs .",
        "test": "echo \"(placeholder)\""
    },
    "peerDependencies": {
        "@aws-amplify/ui-react": ">=6",
        "next": ">=15",
        "react": ">=18",
        "react-dom": ">=18"
    },
    "peerDependenciesMeta": {
        "@types/react": {
            "optional": true
        }
    },
    "dependencies": {
        "@packages/services": "workspace:*",
        "@packages/types": "workspace:*",
        "react-markdown": "10.1.0",
        "remark-gfm": "^4.0.0"
    }
}
